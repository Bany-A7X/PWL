<div class="row">
    <div class="col s3"></div>
    <div class="col s12 left-align">
        {{#each article}}
        <div class="card-panel">
            <h4>{{this.title}}</h4>
            <h5>{{this.body}}</h5>
            <a class="waves-effect waves-light btn" id="btnSignUp" href="{{link}}">Read Article</a>

        </div>

        {{/each}}
    </div>
</div>